{%- set cellslots = v.cellSlots -%}
{%- set code = v.code -%}
{% for c in cellslots -%}
	{%- set start = c.get("start").asText() -%}
	{%- set startInt = c.get("start").asInt() -%}
	{%- set end = c.get("end").asText() -%}
	{%- set endInt = c.get("end").asInt() %}
	<codeslot type="{{c.get("type").asText()}}" id="{{loop.index}}" start="{{start}}" end="{{end}}">
{{ code.substring(startInt, endInt) }}
	</codeslot>
{%- endfor -%}
