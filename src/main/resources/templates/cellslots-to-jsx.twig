{%- set fragments = v.fragments -%}
{%- set code = v.code -%}
{%- set codeindex = 0 -%}
{%- set i = 0 -%}
{% for c in v.cellslots -%}
	{%- set start = c.start-1 -%}
{{ substr(code, codeindex, start) -}}
	{%- if c.type.equals('__fragment') -%}
<>
{{ c.content }}
</>
	{%- else -%}
{{- c.content }}
	{%- endif -%}
	{%- set codeindex = c.end -%}
	{%- set i = i+1 -%}
{%- endfor -%}
{{ substr(code, (v.cellslots[i-1]).end, code.length()) }}